# Simple SDL mini Makefile

CC=gcc

CPPFLAGS= `pkg-config --cflags sdl` -MMD
CFLAGS= -Wall -Wextra -Werror -std=c99 -O3
LDFLAGS= 
LDLIBS= `pkg-config --libs sdl` -lSDL_image -lSDL_gfx -lm

all: main filters

main: main.o
filters: filters.o 


clean:
	${RM} *.o
	${RM} *.d
	${RM} main
	${RM} filters

# END
